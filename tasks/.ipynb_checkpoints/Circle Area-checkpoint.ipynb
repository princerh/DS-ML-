   "source": [
    "def circleArea(r):\n",
    "    return PI*(r**2)\n",
    "\n",
    "PI = 3.14\n",
    "r = float(input(\"Provide the circle radius: \"))\n",
    "area = circleArea(r)\n",
    "area = round(area, 2)\n",
    "print(\"Area: \",area, \"sq. meter\")"
   ]
